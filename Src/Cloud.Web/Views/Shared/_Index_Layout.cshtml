<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/layouts/css/font.css" rel="stylesheet" />
    <link href="~/Content/global/plugins/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/layouts/css/theme.css" rel="stylesheet" />
    <link href="~/Content/global/plugins/jqgrid/jqgrid.css" rel="stylesheet" />
    <link href="~/Content/layouts/css/ui.css" rel="stylesheet" />
    @*留出位置放置样式*@
    @RenderSection("Styles", false)
    <script src="~/Content/global/plugins/jquery/jquery-2.1.1.min.js"></script>
    <script src="~/Content/global/plugins/bootstrap/bootstrap.js"></script>
    <script src="~/Content/global/plugins/jqgrid/jqgrid.min.js"></script>
    <script src="~/Content/global/plugins/jqgrid/grid.locale-cn.js"></script>
    <script src="~/Content/layouts/scripts/cloud.js"></script>
    <script src="~/Content/layouts/scripts/ViewModel.js"></script>
</head>
<body>
    @RenderBody()
    @if (DbRunTimeModule.Enable)
    {
        <div class="panel panel-default">
            <div class="panel-body">
                @Html.Raw(DbRunTimeModule.GetInfo())
            </div>
        </div>
    }
    @*防伪标志*@
    @Html.AntiForgeryToken()
    @*留出位置放置脚本*@
    @RenderSection("Scripts", false)
</body>
</html>
